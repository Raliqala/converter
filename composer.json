{
    "name": "raliqala/any-conversions",
    "description": "Everything conversion like Kilograms to Pounds, temperature, weight,volume, speed, length, angle, time etc..",
    "keywords": [
        "raliqala",
        "any-conversions"
    ],
    "homepage": "https://github.com/raliqala/any-conversions",
    "license": "MIT",
    "authors": [{
        "name": "Topollo",
        "email": "thabohappy@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\Converter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\Converter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
